{
  "name": "@visactor/calculator",
  "version": "2.0.5",
  "description": "SQL-like query executor with DSL",
  "main": "lib",
  "module": "es",
  "types": "es",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "lib",
    "es"
  ],
  "dependencies": {
    "ts-pattern": "~4.1.4",
    "node-sql-parser": "~4.17.0",
    "@visactor/vutils": "~0.19.3"
  },
  "devDependencies": {
    "@types/jest": "~29.5.0",
    "jest": "~29.5.0",
    "jest-environment-jsdom": "~29.5.0",
    "@jest/globals": "~29.5.0",
    "ts-jest": "~29.2.5",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "@types/node": "*",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "10.9.0",
    "typescript": "4.9.5",
    "@internal/jest-config": "0.0.1",
    "@internal/ts-config": "0.0.1",
    "@internal/eslint-config": "0.0.1",
    "@internal/bundler": "0.0.1"
  },
  "scripts": {
    "clean": "rimraf es lib dist build *.tsbuildinfo",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm-run-all clean --parallel build:esm build:cjs"
  }
}